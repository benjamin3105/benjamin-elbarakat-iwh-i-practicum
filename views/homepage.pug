doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Pokemons Custom Objects
        a(href="/update-cobj")  Add/Update this table
        table 
            each pokemons in data
                tr 
                    td Name: #{pokemons.properties.name} 
                    td Type: #{pokemons.properties.type}
                    td Weakness #{pokemons.properties.weakness}